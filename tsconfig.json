{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "sourceMap": true,
    "exactOptionalPropertyTypes": true,
    "noImplicitOverride": true,
    "strict": true,
    "outDir": "./dist",
    "incremental": true,
    "tsBuildInfoFile": "./buildcache"

  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
